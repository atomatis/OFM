{
    "name": "atomatis/ofm",
    "description": "Object file mapper",
    "type": "library",
    "keywords": ["file", "file-manager"],
    "homepage": "https://github.com/atomatis/OFM",
    "require": {
        "php": "8.*",
        "php-school/cli-menu": "^4.2",
        "symfony/property-access": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Atomatis\\OFM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0"
    }
}
